<style>
  .perspective-container {
    width: 100%;
    height: 100vh;
    perspective: 400px;
  }

  .mobile-logo-image {
    bottom: 0px;
    position: fixed;
    left: 0;
    width: 100vw;
    height: 80px;
    z-index: 13;
    background: #f1e9dc;
  }

  .box {
    width: 100%;
    height: 100vh;
    position: relative;
    transform-style: preserve-3d;
    transition: transform 1s;
  }

  .box__face {
    position: absolute;
    width: 100%;
    height: 100%;
  }

  .box__face--front {
    transform: rotateY(0deg) translateZ(50px);
  }
  .box__face--back {
    transform: translate3d(0px, 0px, 0px) scale(1);
    transition: transform 0.3s ease;
  }
  .box__face--right {
    transform: rotateY(90deg) translateZ(50vw);
    border: 2px solid black;
    background: white;
  }
  .box__face--left {
    transform: rotateY(-90deg) translateZ(50vw);
    border: 2px solid black;
    background: white;
  }
  .box__face--top {
    transform: rotateX(90deg) translateZ(50vh);
    border: 0;
    background: white;
  }

  .box__face--bottom {
    transform: rotateX(-90deg) translateZ(50vh);
    border: 0;
    background: white;
  }

  .logo-image {
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%, 30vh);
    width: 100%;
    opacity: 1;
    transition: opacity 0.5s ease;
    /* mix-blend-mode: difference; */
  }

  @media screen and (max-height: 600px) {
    .logo-image {
      transform: translate(-50%, 25vh);
    }
  }

  @media (max-width: 750px) and (min-height: 750px) {
    .logo-image {
      transform: translate(-50%, 35vh);
    }
  }
  
  @media (max-width: 750px) and (min-height: 850px) {
    .logo-image {
      transform: translate(-50%, 40vh);
    }
  }
</style>

<script>
  document.addEventListener("DOMContentLoaded", function () {
      gsap.registerPlugin(ScrollTrigger);

      var section = document.querySelector(".image-container");
      var image = section.querySelector(".box__face--back");
      var logo = section.querySelector(".logo-image"); 

    gsap.timeline({
      scrollTrigger: {
        trigger: section,
        start: "top top",
        end: "100% top",
        pin: true,
        scrub: true,
      },
    });

     gsap.timeline({
      scrollTrigger: {
        trigger: section,
        start: "top",
        end: "175%",
        scrub: true,
      },
    }).to(logo, { top: "10%", width: "20%", ease: "none" });
  }); 
</script>

